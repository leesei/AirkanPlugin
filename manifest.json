{
	"name": "MiLink",
	"version": "1.8",
	"manifest_version": 2,
	// "update_url": "http://pds.duokanbox.com/peer/fetchbyipopen/explorer/upgrade/",

	"description": "MiLink",
	"icons": {
		"48": "48.png"
	},
	"browser_action": {
		"default_icon": "19_2.png", // optional
		"default_title": "MiLink", // optional; shown in tooltip
		"default_location": "sidebar",
		"default_popup": "popup.html" // optional
	},
	"background": {
		"page": "background.html"
	},
	"options_page": "options.html",
	"permissions": ["tabs", "webRequest", "webRequestBlocking", "http://*/*", "https://*/*"]
}
